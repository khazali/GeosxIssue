	<Solvers>
		<CompositionalMultiphaseReservoir
			name="coupledFlowAndWells"
			flowSolverName="compositionalMultiphaseFlow"
			wellSolverName="compositionalMultiphaseWell"
			logLevel="1"
			initialDt="1e4"
			targetRegions="{ reservoir, wellRegion1 }">
			
			<NonlinearSolverParameters
				newtonTol="1.0e-4"
				newtonMaxIter="25"
				timeStepDecreaseIterLimit="0.9"
				timeStepIncreaseIterLimit="0.6"
				timeStepCutFactor="0.1"
				maxTimeStepCuts="10"
				lineSearchAction="None"/>
				<LinearSolverParameters
				solverType="fgmres"
				preconditionerType="mgr"
				krylovTol="1e-4"
				krylovAdaptiveTol="1"
				krylovWeakestTol="1e-2"/>
		</CompositionalMultiphaseReservoir>

		<CompositionalMultiphaseFVM
			name="compositionalMultiphaseFlow"
			targetRegions="{ reservoir }"
			discretization="fluidTPFA"
			temperature="297.15"
			maxCompFractionChange="0.3"
			logLevel="1"
			useMass="1"/>
			
		<CompositionalMultiphaseWell
			name="compositionalMultiphaseWell"
			targetRegions="{ wellRegion1 }"
			maxCompFractionChange="0.5"
			logLevel="1"
			useMass="1">
			<WellControls
				name="wellControls1"
				type="producer"
				control="totalVolRate"
				useSurfaceConditions="1"
				referenceElevation="-2068"
				surfacePressure="101325"
				surfaceTemperature="15.5556"
				targetTotalRate="68"/>			
			<WellControls
				name="wellControls2"
				type="injector"
				control="BHP"
				useSurfaceConditions="1"
				referenceElevation="-2068"
				surfacePressure="101325"
				surfaceTemperature="15.5556"
				targetBHP="32000000"
				targetPhaseRate="60"				
				targetPhaseName="water"/>
		</CompositionalMultiphaseWell>
	</Solvers>

	<Events
		maxTime="1.5e7">
		<PeriodicEvent
			name="MySilo"
			timeFrequency="2e6"
			target="/Outputs/SiloOutput"/>

		<PeriodicEvent
			name="timeHistoryOutput1"
			timeFrequency="1.5e7"
			target="/Outputs/timeHistoryOutput1"/>

		
		<PeriodicEvent
			name="solverApplications"
			maxEventDt="5e5"
			target="/Solvers/coupledFlowAndWells"/>

		<PeriodicEvent
			name="timeHistoryCollection1"
			timeFrequency="1e6"
			target="/Tasks/wellRateCollection1"/>

		<PeriodicEvent
			name="restarts"
			timeFrequency="7.5e6"
			targetExactTimestep="0"
			target="/Outputs/restartOutput"/>    
	</Events>


	<NumericalMethods>
		<FiniteVolume>
			<TwoPointFluxApproximation
				name="fluidTPFA"/>
		</FiniteVolume>
	</NumericalMethods>

	<ElementRegions>
		<CellElementRegion
			name="reservoir"
			cellBlocks="{ hexahedra }"
			materialList="{ fluid, rock, relperm }"/>

		<WellElementRegion
			name="wellRegion1"
			materialList="{ fluid, relperm }"/>
	</ElementRegions>


	<Constitutive>
		<DeadOilFluid
			name="fluid"
			phaseNames="{ oil, water }"
			surfaceDensities="{ 873.5, 1013 }"
			componentMolarWeight="{ 114e-3, 18e-3 }"
			tableFiles="{ PVDO.txt, PVTW.txt }"/>

		<TableRelativePermeability
			name="relperm"
			phaseNames="{ oil, water }"
			wettingNonWettingRelPermTableNames="{ waterRelativePermeabilityTable, oilRelativePermeabilityTable }"/>

		<CompressibleSolidConstantPermeability
			name="rock"
			solidModelName="nullSolid"
			porosityModelName="rockPorosity"
			permeabilityModelName="rockPerm"/>

		<NullModel
			name="nullSolid"/>

		<PressurePorosity
			name="rockPorosity"
			defaultReferencePorosity="0.3"
			referencePressure="101325"
			compressibility="6.0e-12"/>		
	</Constitutive>


	<FieldSpecifications>
		<FieldSpecification
			name="initialPressure"
			initialCondition="1"
			setNames="{ all }"
			objectPath="ElementRegions/reservoir/hexahedra"
			fieldName="pressure"
			scale="100000"
			functionName="pressureFunc"/>

		<FieldSpecification
			name="referencePorosity"
